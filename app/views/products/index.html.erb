<h1><%= t ".title" %></h1>

<%= link_to "New products", new_product_path if authenticated? %>

<div id=products >
  <%= @products.each do |product| %>
  <div>
    <a href="/products/<%= product.id %>">
      <%= link_to product.name, product_path(product.id) %>
    </a>
  </div>
    <% end %>
</div>
